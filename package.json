{
  "name": "preact-boilerplate",
  "version": "1.0.0",
  "description": "A minimal Preact boilerplate project, powered by Parcel.",
  "main": "index.js",
  "scripts": {
    "start": "parcel public/index.html --no-cache --out-dir build/dev --port 3000 --open",
    "build": "parcel build public/index.html --no-cache --out-dir build/prod"
  },
  "keywords": [
    "preact",
    "parcel-bundler",
    "preact boilerplate"
  ],
  "author": {
    "name": "Safdar Jamal",
    "url": "https://safdarjamal.github.io"
  },
  "license": "MIT",
  "dependencies": {
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.0"
  }
}
