{
  "name": "preact-boilerplate",
  "version": "2.0.0",
  "description": "A minimal Preact boilerplate project, powered by Parcel.",
  "scripts": {
    "start": "rimraf build/* && parcel public/index.html --out-dir build --port 3000 --open",
    "build": "rimraf build/* && parcel build public/index.html --out-dir build"
  },
  "keywords": [
    "preact",
    "parcel-bundler",
    "preact boilerplate"
  ],
  "author": {
    "name": "Safdar Jamal",
    "url": "https://safdarjamal.github.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SafdarJamal/preact-boilerplate/issues"
  },
  "homepage": "https://github.com/SafdarJamal/preact-boilerplate#readme",
  "dependencies": {
    "preact": "^10.5.3"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2"
  }
}
